<core:View xmlns:core="sap.ui.core" xmlns="sap.m"
			controllerName="cross.fnd.fiori.inbox.view.MultiSelectDetail" xmlns:html="http://www.w3.org/1999/xhtml"
			resourceBundleName="sap.ca.ui.i18n.i18n" resourceBundleAlias="ca_i18n">
	<Dialog id="DIALOG" contentWidth="25rem" title="{/detailTitle}" state="{/state}" icon="{/icon}">
		<content>
			<TextArea value="{/detailMessage}" editable="false" height="12.5em" width="100%" />
		</content>
		
		<endButton>
			<Button text="{ca_i18n>dialog.ok}" press="onOKPress" />
		</endButton>
	</Dialog>
</core:View>
